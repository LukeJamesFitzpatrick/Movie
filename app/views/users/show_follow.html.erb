<% provide(:title, @title) %>
<div class="row">
  <aside class="col-md-4">
    <section class="user_info">
      <h1><%= @user.username %></h1>
      <span><%= link_to "view my profile", @user %></span>
    </section>
  </aside>
  <div class="col-md-8">
    <h3><%= @title %></h3>
    <% @users.each do |user| %>
      <div class="card-block">
        <div class="row">
          <div class="col-md-12">
            <%= user.username %>
          </div>
        </div>
      </div>
    <% end %>
  </div>
</div>
